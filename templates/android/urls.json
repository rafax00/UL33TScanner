{
    "name": "Hardcoded URLs",
    "type": "regex",
    "content":{
        "url_parameters":"'[&\\?][a-zA-Z0-9\\_]+='",
        "link_finder":"https?:\/\/(www\\.)?[-a-zA-Z0-9@:%._\\+~#=]{1,256}\\.[a-zA-Z0-9()]{1,6}\b([-a-zA-Z0-9()@:%_\\+.~#?&//=]*)",
        "relative_links":"(?:\"|')(((?:/|\\.\\./|\\./)[^\"'><,;| *()(%%$^/\\\\[\\]][^\"'><,;|()]{1,})|([a-zA-Z0-9_\\-/]{1,}/[a-zA-Z0-9_\\-/]{1,}\\.(?:[a-zA-Z]{1,4}|action)(?:[\\?|#][^\"|']{0,}|))|([a-zA-Z0-9_\\-/]{1,}/[a-zA-Z0-9_\\-/]{3,}(?:[\\?|#][^\"|']{0,}|))|([a-zA-Z0-9_\\-]{1,}\\.(?:php|asp|aspx|jsp|json|action|html|js|txt|xml)(?:[\\?|#][^\"|']{0,}|)))(?:\"|')",
        "url": "https:\/\/+[\\w\\-]+\\.+[a-zA-Z]*\/?\\??[^#\n\r]*"
    }
}
